class Student {
    String name;
    int age;
    double cgpa;

    Student(String name, int age, double cgpa) {
        this.name = name;
        this.age = age;
        this.cgpa = cgpa;
    }

    void display() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
        System.out.println("CGPA: " + cgpa);
    }
}

public class Day16StudentClass {
    public static void main(String[] args) {
        Student s = new Student("Vaishnavi", 21, 8.3);
        s.display();
    }
}
